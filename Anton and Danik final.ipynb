    "n = int(input())\n",
    "s = input()\n",
    "countA = 0\n",
    "countD = 0\n",
    "for i in range(n):\n",
    "    if s[i] == \"A\":\n",
    "        countA+=1\n",
    "    if s[i] == \"D\":\n",
    "        countD+=1\n",
    "if countA > countD:\n",
    "    print(\"Anton\")\n",
    "elif countD > countA:\n",
    "    print(\"Danik\")\n",
    "elif countA == countD:\n",
    "    print(\"Friendship\")"
