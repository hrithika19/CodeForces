   "source": [
    "s = input().lower()\n",
    "instr = list(s.strip())\n",
    "lis = []\n",
    "vowels = ('a','e','i','o','u','y')\n",
    "for i in instr:\n",
    "    if i not in vowels:\n",
    "        lis.append(i)\n",
    "s = \"\"\n",
    "for i in lis:\n",
    "    s+= '.' + i\n",
    "print(s)"
   ]
